<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width,initial-scale=1">

	<title>Civilproces</title>

    <link rel="stylesheet" type="text/css" href="/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="/bootstrap/dist/css/bootstrap-theme.min.css">
    <script src="/jquery/dist/jquery.min.js"></script>
    <script src="/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="/ajax/dist/ajax.min.js"></script>

    <script src="/js/shared_functions.js"></script>
    <script src="/js/navbar.js"></script>
    <script src="/js/main.js"></script>

    <link rel="stylesheet" href="/css/style04.css"/>
    <link rel="stylesheet" href="/css/navbar.css"/>
</head>

<body>
    <nav class="navbar" id="top-menu">
        <div class="container-fluid">
            <div class="row">
                <div id="top-menu-box" class="col-xs-12 col-md-12">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse-1" aria-expanded="false" id="top-menu-btn" onclick="menuBtnClick()">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="/"><h2>Civilproces</h2></a>
                    </div>

                    <div class="collapse navbar-collapse" id="navbar-collapse-1">
                        <ul class="nav navbar-nav navbar-right">
                            <li class="navbar-link"><a href="/"><h4>Home</h4></a></li>
                            <li class="navbar-link"><a href="/about"><h4>Om kurserne</h4></a></li>
                            <li class="navbar-link"><a href="/blog"><h4>Blog</h4></a></li>
                            <li class="navbar-link"><a href="/contact"><h4>Kontakt</h4></a></li>
                        </ul>
                    </div><!-- /.navbar-collapse -->
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </nav>


    <div class="container wrapper">

        <div class="row">
            <div id="body_box" class="col-xs-12 col-md-12">
                {{#if success_msg}}
                    <div class="alert alert-success">{{success_msg}}</div>
                {{/if}}

                {{#if error_msg}}
                    <div class="alert alert-danger">{{error_msg}}</div>
                {{/if}}

                {{#if error}}
                    <div class="alert alert-danger">{{error}}</div>
                {{/if}}
                {{{body}}}
            </div>
        </div>

        <div class="row">
            <div id="index-event-box-bottom" class="col-xs-12 hidden-xs hidden-sm hidden-md hidden-lg">
                hello
            </div>
        </div>
    </div> <!-- /container -->

    <div class="container">
        <div class="row">
            <div id="info" class="col-xs-12 col-md-12">
                <p>Mail. Telefon. Andre kontaktinformationer</p>
            </div>
        </div>
    </div>

    <div class="container">
        <div class="row">
            <div id="index-event-box-right" class="col-xs-3 hidden-xs hidden-sm hidden-md hidden-lg">
                hello
            </div>
        </div>
    </div>
</body>
</html>
