<h2>TILMELD</h2>

<form method="post" action="/event:1" id="signupform">
    <div class="form-group">
        <label>Navn</label>
        <input id="name" type="text" class="form-control" placeholder="Fx. Anders Andersen..." name="name">
    </div>
    <div class="form-group">
        <label>Firma</label>
        <input id="company" type="text" class="form-control" placeholder="Firma" name="company">
    </div>
    <div class="form-group">
        <label>Titel</label>
        <input id="working_title" type="text" class="form-control" placeholder="Titel" name="working_title">
    </div>
    <div class="form-group">
        <label>Email</label>
        <input id="email" type="email" class="form-control" placeholder="Email" name="email">
    </div>
    <button type="submit" class="btn btn-default" onclick="saveInputValues()">TILMELD</button>
</form>


<hr>
<h2>TJEK DIN TILMELDING</h2>
<form method="post" action="/check_signup:1">
    <div class="form-group">
        <label>Email</label>
        <input type="email" class="form-control" placeholder="Email" name="email" required>
    </div>
    <button type="submit" class="btn btn-default">TJEK TILMELDING</button>
</form>

